{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/profile", "destination": "/api/profile" },
    { "source": "/api/experiences", "destination": "/api/experiences" },
    { "source": "/api/education", "destination": "/api/education" },
    { "source": "/api/projects", "destination": "/api/projects" },
    { "source": "/api/skills", "destination": "/api/skills" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
